<dataset>
    <variables>
        <variable>
            <name>crs</name>
            <type>int</type>
            <segment_name>CRS</segment_name>
            <attributes>
                <attribute>
                    <name>grid_mapping_name</name>
                    <type>string</type>
                    <value>latitude_longitude</value>
                </attribute>
                <attribute>
                    <name>longitude_of_prime_meridian</name>
                    <type>double</type>
                    <value>0.0</value>
                </attribute>            
                <attribute>
                    <name>semi_major_axis</name>
                    <type>double</type>
                    <value>6378137.0</value>
                </attribute>
                <attribute>
                    <name>inverse_flattening</name>
                    <type>double</type>
                    <value>298.257223563</value>
                </attribute>
            </attributes>
        </variable>
    </variables>
</dataset>