<dataset>
    <variables>
        <variable>
            <name>lat</name>
            <type>double</type>
            <segment_name>VGT_LAT</segment_name>
            <dimensions>
                <id>VGT_LAT</id>
            </dimensions>
            <attributes>
                <attribute>
                    <name>standard_name</name>
                    <type>string</type>
                    <value>latitude</value>
                </attribute>
                <attribute>
                    <name>units</name>
                    <type>string</type>
                    <value>degrees_north</value>
                </attribute>            
                <attribute>
                    <name>valid_min</name>
                    <type>double</type>
                    <value>-56.0</value>
                </attribute>
                <attribute>
                    <name>valid_max</name>
                    <type>double</type>
                    <value>75.0</value>
                </attribute>
                <attribute>
                    <name>bounds</name>
                    <type>string</type>
                    <value>lat_bnds</value>
                </attribute>
            </attributes>
        </variable>
        <variable>
            <name>lon</name>
            <type>double</type>
            <segment_name>VGT_LON</segment_name>
            <dimensions>
                <id>VGT_LON</id>
            </dimensions>
            <attributes>
                <attribute>
                    <name>standard_name</name>
                    <type>string</type>
                    <value>longitude</value>
                </attribute>
                <attribute>
                    <name>units</name>
                    <type>string</type>
                    <value>degrees_east</value>
                </attribute>            
                <attribute>
                    <name>valid_min</name>
                    <type>double</type>
                    <value>-180.0</value>
                </attribute>
                <attribute>
                    <name>valid_max</name>
                    <type>double</type>
                    <value>180.0</value>
                </attribute>
                <attribute>
                    <name>bounds</name>
                    <type>string</type>
                    <value>lon_bnds</value>
                </attribute>
            </attributes>
        </variable>
        <variable>
            <name>lat_bnds</name>
            <type>double</type>
            <segment_name>VGT_LAT_BNDS</segment_name>
            <dimensions>
                <id>VGT_LAT_BNDS</id>
            </dimensions>
        </variable>
        <variable>
            <name>lon_bnds</name>
            <type>double</type>
            <segment_name>VGT_LON_BNDS</segment_name>
            <dimensions>
                <id>VGT_LON_BNDS</id>
            </dimensions>
        </variable>
    </variables>
</dataset>